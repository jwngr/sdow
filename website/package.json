{
  "name": "sdow",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "dependencies": {
    "d3": "^7.9.0",
    "lodash": "^4.17.21",
    "react": "^19.2.0",
    "react-autosuggest": "^10.1.0",
    "react-dom": "^19.2.0",
    "react-modal": "^3.16.3",
    "react-router-dom": "^6.30.0",
    "styled-components": "^6.1.19",
    "typeface-crimson-text": "1.1.13",
    "typeface-quicksand": "1.1.13"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.7.0",
    "@types/d3": "^7.4.3",
    "@types/lodash": "^4.17.20",
    "@types/node": "^24.7.2",
    "@types/react": "^19.2.2",
    "@types/react-autosuggest": "^10.1.11",
    "@types/react-dom": "^19.2.1",
    "@types/react-modal": "^3.16.3",
    "@vitejs/plugin-react": "^5.0.4",
    "eslint": "^9.37.0",
    "eslint-plugin-react-hooks": "^7.0.0",
    "globals": "^16.4.0",
    "prettier": "^3.6.2",
    "source-map-explorer": "^2.5.3",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.0",
    "vite": "^7.1.9"
  },
  "scripts": {
    "analyze": "source-map-explorer build/static/js/*",
    "lint": "prettier --check 'src/**/*.{js,jsx,ts,tsx,json,css}' && echo 'Running ESLint...' && eslint src --report-unused-disable-directives --max-warnings 0 && echo 'All files pass linting!'",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,json,css}'",
    "start": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "update-deps": "npx npm-check-updates -u --reject 'react-router-dom' && npm install"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
