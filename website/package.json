  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.4.0",
    "@types/d3": "^7.4.3",
    "@types/lodash": "^4.17.13",
    "@types/node": "^22.10.3",
    "@types/react": "^18.3.12",
    "@types/react-autosuggest": "^10.1.11",
    "@types/react-dom": "^18.3.1",
